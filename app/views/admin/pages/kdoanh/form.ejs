<%- include('./../../helpers/selectbox') %>
<%- include('./../../helpers/image') %>
<% 
    const collection = 'kdoanh';
    const linkPrefix = systemConfig.prefixAdmin + `/${collection}`;
    const folderUpload = 'uploads' + `/${collection}/`;
    const link_no_avatar = folderUpload +'no_avatar.png' ; 
%>

<div class="row">
    <div class="col-lg-12">
        <div class="panel panel-default">
            <!--  neeus cos loix thi mow show ra -->
            <% if (notify.err) { %>  
                <div class="alert alert-danger role="alert"  >
                    <span class="close-btn">&times;</span> 
                   <%= notify.err %>
                </div>
             <% } %>
             <!--  -->
            <div class="panel-heading">
                Form 
            </div>
            <div class="panel-body"> 
                <div class="row">
                    <div class="col-lg-12">
                        <form role="form" action="<%= linkPrefix %>/save" method="POST"  > 
                         
                            <div class="form-group">
                                <label>Điện lực</label>
                                <select name="name" class="form-control" >
                                    <!-- <option value="Novalue">Chọn trạng thái</option> -->
                                    <option value="PC12AA">PC12AA</option>
                                    <option value="PC12BB">PC12BB</option>
                                    <option value="PC12CC">PC12CC</option>
                                    <option value="PC12DD">PC12DD</option>
                                    <option value="PC12EE">PC12EE</option>
                                    <option value="PC12GG">PC12GG</option>
                                    <option value="PC12HH">PC12HH</option>
                                    <option value="PC12II">PC12II</option>
                                    <option value="PC12JJ">PC12JJ</option>
                                    <option value="PC12KK">PC12KK</option>
                                    <option value="PC12LL">PC12LL</option>
                                    <option value="PC12MM">PC12MM</option>
                                    <option value="PC12NN">PC12NN</option>
                                    <option value="PC12PP">PC12PP</option>
                                    <option value="PC12">PC12</option>
                                    <!-- <option value="Inactive"  <%= (data.status == "Inactive") ? 'selected="selected"' : '' %> >Inactive</option>                                     -->
                                </select>
                            </div>

                           
                            <div class="form-group">
                                <label>Số lượng khách hàng</label>
                                <input class="form-control" name="khang" value="<%= data.khang %>" >
                            </div>
                            <div class="form-group">
                                <label>Sản lượng thương phẩm</label>
                                <input class="form-control" name="tpham" value="<%= data.tpham %>" >
                            </div>
                             
                        
                            </br>                                                     

                            <div>
                                <input class="form-control" type="hidden" name="id" value="<%= data.id %>"   >
                                <button type="submit" class="btn btn-primary">Submit </button>
                                <a type="button" href="<%= linkPrefix %>" class="btn btn-default">Cancel</a>                                
                            </div>

                        </form>


                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
 

